generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Item {
  id        Int      @id @default(autoincrement())
  nome      String
  descricao String
  valor     Float
  imagem    String
  createdAt DateTime @default(now()) @map("created_at")
}

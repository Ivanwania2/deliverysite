datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
generator client {
  provider = "prisma-client-js"
}

model Item {
  id        Int     @id @default(autoincrement())
  nome      String
  descricao String
  valor     Float
  imagem    String
}
